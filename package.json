{
  "name": "typescript-start",
  "version": "0.6.0",
  "description": "A start boiler template for typescript projects",
  "main": "src/js/app.ts",
  "scripts": {
    "buildRefs": "tsc -p common",
    "build": "npm run build:development",
    "build:development": "gulp --env=development",
    "build:production": "gulp --env=production",
    "server": "lite-server --baseDir ./dist",
    "start": "gulp watch --env=development",
    "deploy:development": "npm run build:development && firebase deploy",
    "deploy:production": "npm run build:production && firebase deploy"
  },
  "repository": {
